<!DOCTYPE HTML>

<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>News mApp</title>
    <!-- BOOTSTRAP CSS -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <!-- BOOTSTRAP THEME CSS -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap-theme.min.css">
    <!-- CSS STYLE SHEET -->
    <link rel="stylesheet" href="style.css"/>
    <!-- LEAFLET CSS -->
    <link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.7.3/leaflet.css"/>
    <!-- MODERNIZR -->
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.2/modernizr.min.js"></script>
</head>

<body>

    <!-- TITLE HEADER -->

    <div class="container">
        <div class="row">
            <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8" id="title">news mApp</div>
                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 pull-right">
                    <canvas class="pull-right" id="icon1"></canvas>
                    <div class="pull-right" id="temp"></div>
                </div>
            </div>
        </div>
    </div>

    <!-- TOP BAR -->

    <div class="container">
        <div class="row" id="top-bar">
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6" id="city"></div>
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6" id="date"></div>
        </div>
    </div>

    <!-- MAP AND NEWS CONTAINER -->

    <div class="container section-one">
        <div class="row">
            <div class="col-sm-12 col-md-6 col-lg-6" id="map"></div>
            <div class="col-sm-12 col-md-6 col-lg-6" id="newsSpace"></div>
        </div>
    </div>

    <!-- BACKGROUND INFORMATION CONTAINER -->

    <div class="container section-two">
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12" id="backgroundSpace"></div>
        </div>
    </div>

    <!-- CITY TEMPLATE -->

    <script type="text/template" id="cityTemplate">
        <%= city %>, <%= country %>
    </script>

    <!-- WEATHER TEMPLATE -->

    <script type="text/template" id="tempTemplate">
        <%= temp%>Â°F
    </script>

    <!-- COUNTRY BACKGROUND TEMPLATE -->

    <script type="text/template" id="backgroundTemplate">
        <h3>Background Information</h3>
        <%= backgroundInfo.extract %>
    </script>

    <!-- NEWS TEMPLATE -->

    <script type="text/template" id="newsTemplate">
        <h2><u><%= country %></u></h2>
          <h4>Recent News</h4>
            <ul>
            <% _.each(stories, function (story) { %>
                <li><a href="<%= story.link[1].$text %>"><%= story.title.$text %></</a></li>
            <% }); %>
            </ul>

    </script>

    <!-- JQUERY -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <!-- UNDERSCORE JS -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.6.0/underscore-min.js"></script>
    <!-- SKYCON JS -->
    <script src="http://learn.themakersquare.com/materials/week-7/weather-icons.js"></script>
    <!-- LEAFLET JS -->
    <script src="http://cdn.leafletjs.com/leaflet-0.7.3/leaflet.js"></script>

    <!-- GeoJSON COUNTRY DATA -->
    <script src="world.js"></script>
    <!-- DATE JS -->
    <script src="date.js"></script>
    <!-- MAP LAYER DATA -->
    <script src="map.js"></script>
    <!-- GeoJSON LAYER DATA -->
    <script src="geoJSONLayer.js"></script>
    <!-- API CALLS -->
    <script src="apiCalls.js"></script>
    <!-- TEMPLATE RENDERING -->
    <script src="templateRendering.js"></script>

    <!-- BOOTSTAP JS -->
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
</body>

</html>